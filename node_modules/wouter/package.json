    "./static-location": {
      "require": "./cjs/static-location.js",
      "default": "./static-location.js"
    }
  },
  "files": [
    "*.js",
    "*.d.ts",
    "preact",
    "cjs"
  ],
  "homepage": "https://github.com/molefrog/wouter#readme",
  "husky": {
    "hooks": {
